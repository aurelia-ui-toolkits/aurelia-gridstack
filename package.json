    "copyfiles": "^2.4.1",
    "css-loader": "^6.10.0",
    "eslint": "^8.56.0",
    "eslint-plugin-compat": "^4.2.0",
    "eslint-plugin-import": "^2.29.1",
    "gridstack": "^10.1.0",
    "html-loader": "^5.0.0",
    "html-webpack-plugin": "^5.6.0",
